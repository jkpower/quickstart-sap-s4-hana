            "MinLength": "8",
            "NoEcho": "true",
            "Type": "String"
        },
        "HostedZoneName": {
            "Description": "A *new* Route 53 privately hosted zone created for all deployed servers. Use a fully-qualified domain name (e.g., mycompany.local).",
            "Type": "String",
            "MinLength": "1",
            "MaxLength": "64",
            "AllowedPattern": "(?!-)[a-zA-Z0-9-.]*(?<!-)",
            "ConstraintDescription": "must be a valid fully-qualified domain name."
        },
        "InstallRDPInstance": {
            "AllowedValues": [
                "Yes",
