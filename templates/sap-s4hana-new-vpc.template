        },
        "PublicSubnet1CIDR": {
            "AllowedPattern": "^(([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])(\\/(1[6-9]|2[0-8]))$",
            "ConstraintDescription": "CIDR block for the public (DMZ) subnet 1 located in Availability Zone 1. CIDR block parameter must be in the form x.x.x.x/16-28",
            "Default": "10.0.128.0/20",
            "Description": "CIDR block for the public (DMZ) subnet 1 located in Availability Zone 1.",
            "Type": "String"
        },
        "PublicSubnet1ID": {
            "Description": "The ID of the existing public subnet 1 where your Linux bastion host is deployed, and where RDP will be deployed.",
            "Type": "AWS::EC2::Subnet::Id"
        },
        "PublicSubnet2CIDR": {
            "AllowedPattern": "^(([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])(\\/(1[6-9]|2[0-8]))$",
            "ConstraintDescription": "CIDR block parameter must be in the form x.x.x.x/16-28",
            "Default": "10.0.144.0/20",
            "Description": "CIDR block for the public (DMZ) subnet 2 located in Availability Zone 2. CIDR block for the public (DMZ) subnet 2 located in Availability Zone 2",
            "Type": "String"
        },
        "QSS3BucketName": {
            "AllowedPattern": "^[0-9a-zA-Z]+([0-9a-zA-Z-]*[0-9a-zA-Z])*$",
            "ConstraintDescription": "Quick Start bucket name can include numbers, lowercase letters, uppercase letters, and hyphens (-). It cannot start or end with a hyphen (-).",
            "Default": "aws-quickstart",
            "Description": "S3 bucket name for your copy of the Quick Start assets. This string can include numbers, lowercase letters, uppercase letters, and hyphens (-), but it cannot start or end with a hyphen (-).",
            "Type": "String"
        },
        "QSS3KeyPrefix": {
            "AllowedPattern": "^[0-9a-zA-Z-/]*$",
            "ConstraintDescription": "Quick Start key prefix can include numbers, lowercase letters, uppercase letters, hyphens (-), and forward slash (/).",
            "Default": "quickstart-sap-s4hana/",
            "Description": "S3 key name prefix used to simulate a folder for your copy of the Quick Start assets. This prefix can include numbers, lowercase letters, uppercase letters, hyphens (-), and forward slashes (/).",
			"Type": "String"
        },
        "RDPInstanceType": {
            "AllowedValues": [
                "c4.large",
                "c4.xlarge",
                "m4.large",
                "m4.xlarge"
            ],
            "Default": "c4.large",
            "Description": "Amazon EC2 instance type for the Windows RDP instance.",
            "Type": "String"
        },
        "SAPAASHostname": {
            "Default": "sidapp00",
            "Description": "Host name *template* to use for SAP AAS.",
            "Type": "String"
        },
        "SAPHANAHostname": {
            "Default": "siddbmas",
            "Description": "Host name to use for the SAP HANA database server.",
            "Type": "String"
        },
        "SAPInstallMediaBucket": {
            "Default": "",
            "Description": "Name of your Amazon S3 bucket for your SAP S/4HANA software. Do *not* include s3:// (enter only the bucket name; e.g., my-sw-bucket).",
            "Type": "String"
        },
        "SAPInstallMediaKeyPrefix": {
            "Default": "",
            "Description": "Name of your S3 key prefix for your SAP S/4HANA software (e.g., my/sw/version). Leave blank if you do not use a key prefix.",
            "Type": "String"
        },
        "SAPInstanceNum": {
            "AllowedPattern": "([0-8]{1}[0-9]{1}|[9]{1}[0-7]{1})",
            "Default": "00",
            "Description": "SAP instance number to use for HANA database and S/4HANA ABAP installation and setup, and to open ports for security groups.",
            "Type": "String"
        },
        "SAPPASHostname": {
            "Default": "sidpas00",
            "Description": "Host name (DNS short name) to use for the SAP PAS server (e.g., sidpas00).",
            "Type": "String"
        },
        "SAPTZ": {
            "AllowedValues": [
                "PT",
                "CT",
                "ET",
                "UC"
            ],
            "ConstraintDescription": "This value must consist of 2 characters.",
            "Default": "UC",
            "Description": "The time zone of your SAP server (PT, CT, ET, or UC)",
            "Type": "String"
        },
        "SAPSchemaName": {
            "Default": "SAPHANADB",
            "Description": "SAP ABAP schema name for HANA database.",
            "Type": "String"
        },
        "SID": {
            "AllowedPattern": "([A-Z]{1}[0-9A-Z]{2})",
            "ConstraintDescription": "This value must consist of 3 characters.",
            "Default": "HDB",
            "Description": "HANA database system ID for installation and setup.",
            "Type": "String"
        },
        "SAPSID": {
            "AllowedPattern": "([A-Z]{1}[0-9A-Z]{2})",
            "ConstraintDescription": "This value must consist of 3 characters.",
            "Default": "S4H",
            "Description": "S/4HANA ABAP system ID for installation and setup. (For Standard installation, SAPSID must be different from SID.)",
            "Type": "String"
        },
        "SIDadmUID": {
            "Default": "1002",
            "Description": "User ID for the SIDadm user.",
            "Type": "String"
        },
        "SLESBYOSRegCode": {
            "Type": "String",
            "Default": "",
            "Description": "Registration code for SUSE BYOS (if you are using the BYOS option)."
        },
        "VolumeTypeHanaData": {
            "Type": "String",
            "Description": "Amazon EBS volume type for the HANA data. Choose General Purpose SSD (gp2) or Provisioned IOPS SSD (io1).",
            "Default": "gp2",
            "AllowedValues": [
                "gp2",
                "io1"
            ]
        },
        "VolumeTypeHanaLog": {
            "Type": "String",
            "Description": "Amazon EBS volume type for SAP HANA log. Choose General Purpose SSD (gp2) or Provisioned IOPS SSD (io1).",
            "Default": "gp2",
            "AllowedValues": [
                "gp2",
                "io1"
            ]
        },
        "VPCID": {
            "Description": "The existing Amazon VPC where you want to deploy SAP App Server Cluster.",
            "Type": "AWS::EC2::VPC::Id"
        }
